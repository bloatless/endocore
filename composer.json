{
    "name": "bloatless/endocore",
    "description": "Bloatless Endocore is a framework designed to quickly build web applications following the Action-Domain-Responder pattern.",
    "keywords": [
        "framework",
        "adr",
        "action domain responder",
        "bloatless"
    ],
    "license": "MIT",
    "homepage": "https://bloatless.org",
    "authors": [
        {
            "name": "Simon Samtleben",
            "email": "foo@bloatless.org"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [{
        "type": "path",
        "url": "../endocore*",
        "symlink": true
    }],
    "autoload": {
        "psr-4": {
            "Bloatless\\Endocore\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "league/container": "^3.3",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    }
}
